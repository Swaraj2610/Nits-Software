  <script>
  $(document).ready(function () {
    // Set the interval for auto sliding (in milliseconds)
    var interval = 5000; // Adjust as needed

    // Function to slide to the next review
    function slideNext() {
      var $activeReview = $('.ti-review-item.active');
      var $nextReview = $activeReview.next('.ti-review-item');

      if ($nextReview.length === 0) {
        // If there is no next review, go back to the first one
        $nextReview = $('.ti-review-item:first');
      }

      // Hide the current active review and show the next one
      $activeReview.removeClass('active');
      $nextReview.addClass('active');
    }

    // Set up the interval to automatically slide to the next review
    setInterval(slideNext, interval);
  });
</script>
